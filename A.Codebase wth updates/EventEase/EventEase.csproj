    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>d46e167c-a13c-46fc-a805-70c895fe3699</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Azure.Storage.Blobs" Version="12.26.0-beta.1" />
